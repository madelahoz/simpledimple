{
    "width": 590,
    "height": 400,
    "bounds": {
        "x": 20,
        "y": 20,
        "width": 550,
        "height": 300
    },
    "axes": [
        {
            "position": "x",
            "categoryFields": ["BarX"],
            "hidden": true
        },
        {
            "position": "y",
            "measure": ["BarY"],
            "overrideMax": 100,
            "hidden": true
        },
        {
            "position": "x",
            "measure": ["BubX"],
            "overrideMax": 60,
            "hidden": true
        },
        {
            "position": "y",
            "measure": ["BubY"],
            "overrideMax": 100,
            "hidden": true
        }
    ],
    "series": [
        {
            "categoryFields": ["Seg"],
            "plot": "bar",
            "axes": [0, 1],
            "barGap": 0.99
        },
        {
            "categoryFields": ["Ball"],
            "plot": "bubble",
            "axes": [2, 3]
        }
    ],
    "assignedColors": [
        {
            "tag": "G",
            "fill": "#FFF",
            "stroke": "#FFF",
            "opacity": 0
        },
        {
            "tag": "P",
            "fill": "#888",
            "stroke": "#888",
            "opacity": 1
        },
        {
            "tag": "Ball",
            "fill": "#888",
            "stroke": "#888",
            "opacity": 1
        }
    ],
    "storyboard": {
        "categoryFields": "F",
        "frameDuration": 600
    },
    "afterDraw": "var sb = simpledimple.chart.storyboard;\nsb.storyLabel.remove();\n"
}
