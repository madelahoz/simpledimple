{
    "width": 590,
    "height": 400,
    "bounds": {
        "x": 70,
        "y": 40,
        "width": 490,
        "height": 320
    },
    "axes": [
        {
            "position": "x",
            "timeField": "Day",
            "inputFormat": "%d %b %Y",
            "outputFormat": "%d %b",
            "overrideMin": "2010-06-20",
            "overrideMax": "2011-01-02",
            "timePeriod": "weeks",
            "timeInterval": 4
        },
        {
            "position": "y",
            "timeField": "Time of Day",
            "inputFormat": "%Y-%m-%d %H:%M",
            "outputFormat": "%H:%M",
            "overrideMin": "01/01/2000 9:00 am",
            "overrideMax": "01/01/2000 6:00 pm"
        },
        {
            "position": "z",
            "measure": "Value",
            "overrideMin": 900,
            "overrideMax": 3000
        }
    ],
    "series": [
        {
            "categoryFields": ["Shift"],
            "plot": "bubble"
        },
        {
            "categoryFields": ["Shift"],
            "plot": "line",
            "lineMarkers": true
        }
    ],
    "legends": [
        {
            "x": 180,
            "y": 10,
            "width": 360,
            "height": 20,
            "horizontalAlign": "right"
        }
    ]
}
